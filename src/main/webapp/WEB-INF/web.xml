<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>BlockingServlet</servlet-name>
		<servlet-class>test.blocking.HelloServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsyncServlet</servlet-name>
		<servlet-class>test.asyncservlet.AsyncHelloServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet>
		<servlet-name>AsyncPureHelloServlet</servlet-name>
		<servlet-class>test.asyncservlet.AsyncPureHelloServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet>
		<servlet-name>HelloServlet</servlet-name>
		<servlet-class>test.HelloService</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet>
		<servlet-name>BlockingClockServlet</servlet-name>
		<servlet-class>test.blocking.ClockServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsyncClockServlet</servlet-name>
		<servlet-class>test.asyncservlet.AsyncClockServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsyncPureHelloServlet</servlet-name>
		<url-pattern>/hello2</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BlockingServlet</servlet-name>
		<url-pattern>/test1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsyncServlet</servlet-name>
		<url-pattern>/test2</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BlockingClockServlet</servlet-name>
		<url-pattern>/clock1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsyncClockServlet</servlet-name>
		<url-pattern>/clock2</url-pattern>
	</servlet-mapping>
</web-app>
