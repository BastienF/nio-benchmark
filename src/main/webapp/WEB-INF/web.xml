<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>HelloServlet</servlet-name>
		<servlet-class>com.octo.niobenchmark.servlet.blocking.HelloServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsyncHelloServlet</servlet-name>
		<servlet-class>com.octo.niobenchmark.servlet.async.AsyncHelloServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet>
		<servlet-name>TestServlet</servlet-name>
		<servlet-class>com.octo.niobenchmark.servlet.blocking.TestServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsyncTestServlet</servlet-name>
		<servlet-class>com.octo.niobenchmark.servlet.async.AsyncTestServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsyncHelloServlet</servlet-name>
		<url-pattern>/helloasync</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TestServlet</servlet-name>
		<url-pattern>/test1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AsyncTestServlet</servlet-name>
		<url-pattern>/test2</url-pattern>
	</servlet-mapping>
</web-app>
